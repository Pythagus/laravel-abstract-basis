{
    "name": "pythagus/laravel-abstract-basis",
    "description": "A collection of the main Laravel objects for saving time",
    "keywords": ["Laravel", "abstract"],
    "homepage": "https://github.com/Pythagus/laravel-abstract-basis",
    "type": "package",
    "license": "MIT",
    "authors": [{
        "name": "Damien Molina",
        "email": "opensource@dmolina.fr"
    }],
    "require": {
        "php": ">=7.0",
        "ext-curl": "*",
        "ext-json": "*",
        "htmlmin/htmlmin": "^9.0",
        "laravel/framework": ">=5.3",
        "laracasts/flash": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Pythagus\\LaravelAbstractBasis\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pythagus\\LaravelAbstractBasis\\AbstractBasisServiceProvider"
            ]
        }
    }
}
